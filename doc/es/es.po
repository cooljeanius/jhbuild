"&lt;repository type=\"cvs\" name=\"tango.freedesktop.org\"\n"
"    cvsroot=\":pserver:anoncvs@anoncvs.freedesktop.org:/cvs/tango\"\n"
"    password=\"\"/&gt;\n"
"        "

